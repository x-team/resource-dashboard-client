<h3 class="list-group-item-heading">{{model.name}}</h3>
<div class="list-group-item-text">
  {{#if isOpportunityType}}
    <time>
      {{model.dateFrom}}
      {{#if model.dateTo}}- {{model.dateTo}}{{/if}}
    </time>
  {{/if}}

  {{#if isDeveloperType}}
    <time>{{model.createdAt}}</time>
  {{/if}}

  <ul>
    {{#each model.skills as |skill|}}
      <li>{{skill}}</li>
    {{/each}}
  </ul>
</div>
